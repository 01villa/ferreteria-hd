datasource db {
  provider = "postgresql" // Cambia esto seg√∫n tu base de datos
  url      = env("DATABASE_URL")
}
generator client {
  provider = "prisma-client-js"
}model Customer {
  id           String       @id @default(uuid()) @db.Uuid
  customerName String
  contactName  String
  contactPhone String
  contactEmail String
  address      String?
  isActive     Boolean      @default(true)
  createdAt    DateTime     @default(now())
  updatedAt    DateTime     @updatedAt
}